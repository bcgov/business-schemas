{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://bcrs.gov.bc.ca/.well_known/schemas/special_resolution",
    "definitions": {},
    "required": [
        "specialResolution"
    ],
    "type": "object",
    "title": "Special Resolution",
    "properties": {
        "specialResolution": {
            "type": "object",
            "required": [
                "resolution"
            ],
            "properties": {
                "meetingDate": {
                    "type": "string",
                    "format": "date"
                },
                "resolution": {
                    "type": "string"
                },
                "resolutionFileKey": {
                    "type": "string",
                    "title": "The Identifier for special resolution file in file server"
                },
                "resolutionFileName": {
                    "type": "string",
                    "title": "The file name while uploading"
                }
            }
        }
    }
}
